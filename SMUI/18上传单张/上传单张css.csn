<snippet name="上传单张css" description="">
<insertText location="beforeSelection">
<![CDATA[/*上传单张图片*/
.uploader-wrap{
	position: relative;
	background-color: #fff;
	padding: 10px;
	min-height: 100px;
	overflow: hidden;
}
.uploader-pic li{
	position: absolute;
	top: 10px;
	left: 0;
	z-index: 1;
	width: 80px;
	height: 80px;
	margin: 0 5px 10px;
	background-color: #ccc;
}	
.uploader-file {
	position: absolute;
	top: 10px;
	left: 0;
	z-index: 3;
	display: block;
	width: 80px;
	height: 80px;
	margin: 0 5px;
	border: 1px solid #D9D9D9;
	background-image: url(images/icon-plus.png);
	background-size: 25px !important;	
}
.uploader-file input[type=file]{
	position: absolute;
	z-index: 2;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	opacity: 0;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}	]]>
</insertText>
<insertText location="afterSelection"><![CDATA[]]>
</insertText>
</snippet>
