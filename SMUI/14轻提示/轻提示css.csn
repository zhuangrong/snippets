<snippet name="轻提示css" description="">
<insertText location="beforeSelection">
<![CDATA[/*轻提示*/
.toast-mask {
	position: fixed;
	z-index: 1000;
	top: 0;
	right: 0;
	left: 0;
	bottom: 0;
}	
.toast {
	position: fixed;
	top: 180px;
	left: 50%;
	z-index: 5000;
	width: 122px;
	min-height: 122px;
	margin-left: -60px;
	background: rgba(17, 17, 17, 0.7);
	text-align: center;
	border-radius: 5px;
	color: #fff;
}
.toast i{
	display: block;
	width: 38px;
	height: 38px;
	margin: 30px auto 5px;
	vertical-align: baseline;
}
.loading {
	-webkit-animation: loading 1s steps(12, end) infinite;
	background: transparent url(data:image/svg+xml;base64,) no-repeat;
	background-size: 100%;
}
@-webkit-keyframes loading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}]]>
</insertText>
